// conditional statement with prompt and alert
//prompt ask something with user where as alert shows a message to user

let a= prompt("Enter your age");

if(a>18){
    alert("Welcome!");
}
else{
    alert("You are under age");
}


// code for confirmation
let z= confirm("Are you sure you want to delete this ?");

if(z){
    console.log(" Iteam Deleted ");
}
else{
    console.log("Deletion canceled");
}
